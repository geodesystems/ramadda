<!-- 
Since this is for import add a:
<import resource="type_assets_physical.xml"/>
into some types.xml file
-->
<type  name="type_assets_physical" 
 description="Physical Asset" 
 handler="org.ramadda.repository.type.ExtensibleGroupTypeHandler" 
 super="type_assets_base" 
 supercategory="General" 
 category="Assets" 
foruser="false"
>

<!-- Columns-->
<column  name="manufacturer"  label="Manufacturer"  type="enumerationplus"  enumerationspecifictotype="true" cansearch="true" />
<column  name="model"  label="Model"  type="enumerationplus"  size="400" cansearch="true" enumerationspecifictotype="true" />
<column  name="model_year"  label="Model year"  type="int"  cansearch="true" />
<column  name="serial_number"  label="Serial Number"  type="string"  size="400" cansearch="true" />
<!-- Properties-->
<property name="record.file.class" value="org.ramadda.data.point.text.CsvFile"/>
<property  name="icon"  value="/assets/physical.png" />
<property  name="date.nullok"  value="true" />
</type>
