     <api request="/swagger/api-docs"           method="processSwaggerApiRequest"  admin="false"/>
     <api request="/swagger/api-docs/type/*"           method="processSwaggerTypeRequest"  admin="false"/>
     <api request="/swagger/api-docs/service/*"       method="processSwaggerServiceRequest"  admin="false"/>
     <api request="/swagger/api-docs/point"           method="processSwaggerPointRequest"  admin="false"/>
     <api request="/swagger/api-docs/gridaspoint"     method="processSwaggerGridAsPointRequest"  admin="false"/>
     <api request="/swagger/api-docs/gridsubset"     method="processSwaggerGridSubsetRequest"  admin="false"/>
</api>
