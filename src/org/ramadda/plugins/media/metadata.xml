<?xml version="1.0" encoding="ISO-8859-1"?>
<metadata>
  <handler category="YouTube" >
    <type id="media_video_cue" name="Video Cue" entrytype="media_youtubevideo">
      <element id="name" name="Name"/>
      <element id="minutes" name="Offset" datatype="double" suffix="(Minutes)"/>
    </type>
  </handler>

  <handler category="Miscellaneous">
    <type name="Document Comment" displaycategory="Comments" id="media_comment"  searchable="false" browsable="true">
      <element name="Location"  searchable="false"/>
      <element name="Comment"  searchable="false" rows="5"/>
      <element name="Screen shot" datatype="file" thumbnail="false"/>
      <template type="xxxhtml"><![CDATA[<b>${attr1}</b><br>${attr2}]]></template>
    </type>
    <type name="3D Label" id="3d_label"  searchable="false" browsable="true">
      <element name="Label" />
      <element name="Width"  default="6" suffix=""/>      
      <element name="Position"  default="-1 1 -1" suffix="X Y Z"/>
      <element name="Rotation"  default="0 0 0" suffix=""/>            
    </type>


</handler>
</metadata>
