<!-- 
Since this is for import add a:
<import resource="type_archive_paleontology_specimen.xml"/>
into some types.xml file
-->
<type  name="type_archive_paleontology_specimen" 
 description="Paleontology Specimen" 
 handler="org.ramadda.repository.type.GenericTypeHandler" 
 super="type_archive_geologic_specimen" 
>

<!-- Columns-->
<column  name="some_column"  label="Some Column"  type="string"  group="Paleontology" />
<!-- Properties-->
<property name="record.file.class" value="org.ramadda.data.point.text.CsvFile"/>
<property  name="icon"  value="/archive/bone.png" />
<property  name="html.tabs"  value="true" />
<property  name="column.showenumerationpopup"  value="true" />
<property  name="column.numberformat"  value="#0.0" />
</type>
