<tables>

<table id="investigation_oldphonelog" name="Old Phone Log" category="Investigation" icon="/investigation/telephone.png" handler="com.ramadda.plugins.investigation.OldPhoneDbTypeHandler">
   <column name="time" type="datetime"   label="Date"  cansearch="true" canlist="true" editable="false" format="MMM d yyyy h:mm a">
   </column>

   <column name="from_name" type="enumerationplus" label="From Name"   cansearch="true" canlist="true" editable="true"/>
   <column name="from_number" type="string" label="From Number"   cansearch="true" canlist="true" editable="false">
       <property name="label" value="true"/>
   </column>
`   <column name="from_city"    type="string"   label="From City"  cansearch="true" canlist="true" editable="false"/>
   <column name="from_state"    type="enumeration"   label="From State"  cansearch="true" canlist="true" editable="false" columns="5" values="file:/com/ramadda/plugins/investigation/resources/states.txt"/>


   <column name="to_name" type="enumerationplus" label="To Name"   cansearch="true" canlist="true" editable="true"/>
   <column name="to_number" type="string" label="To Number"   cansearch="true" canlist="true" editable="false">
       <property name="label" value="true"/>
   </column>
`   <column name="to_city"    type="string"   label="To City"  cansearch="true" canlist="true" editable="false"/>
   <column name="to_state"    type="enumeration"   label="To State"  cansearch="true" canlist="true" editable="false" columns="5" values="file:/com/ramadda/plugins/investigation/resources/states.txt"/>

   <column name="tag"    type="enumerationplus"   label="Tag"  cansearch="true" canlist="true" values="default,important">
   </column>
   <column name="notes"    type="string"   label="Notes"  cansearch="true" canlist="true" rows="5"/>
</table>



</tables>
